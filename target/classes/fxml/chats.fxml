                  <Image url="@../images/closeChat.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
    </children>
   <stylesheets>
      <URL value="@../stylesheets/font.css" />
      <URL value="@../stylesheets/chatList.css" />
   </stylesheets>
