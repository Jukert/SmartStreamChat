            <VBox fx:id="chatList" prefHeight="598.0" prefWidth="499.0" style="-fx-background-color: rgb(124,252,0);" />
         </content>
      </ScrollPane>
        <Pane fx:id="upBorder" prefHeight="32.0" prefWidth="500.0">
         <children>
            <ImageView fx:id="btnClose" fitHeight="26.0" fitWidth="28.0" layoutX="470.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
